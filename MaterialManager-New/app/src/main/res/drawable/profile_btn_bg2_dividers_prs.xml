<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <solid android:color="@color/profile_icon_btn_shadow_color_prs" />
            <corners android:radius="0dp" />
            <padding
                android:bottom="10dp"
                android:top="10dp" />
        </shape>
    </item>
    <item
        android:bottom="-10dp"
        android:top="-9dp">
        <shape android:shape="rectangle">
            <solid android:color="@color/profile_icon_btn_bg_color_prs" />
            <corners android:radius="0dp" />
        </shape>
    </item>
    <item
        android:bottom="-12dp"
        android:top="-12dp">
        <shape>
            <solid android:color="@android:color/transparent" />
            <stroke
                android:width="1dp"
                android:color="@color/dividers" />
        </shape>
    </item>
</layer-list>