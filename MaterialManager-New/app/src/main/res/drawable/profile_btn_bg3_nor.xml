<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <solid android:color="@color/profile_icon_btn_shadow_color_nor" />
            <corners
                android:bottomLeftRadius="0dp"
                android:radius="6dp"
                android:topLeftRadius="0dp"
                android:topRightRadius="0dp" />
            <padding
                android:bottom="10dp"
                android:top="10dp" />
        </shape>
    </item>
    <item
        android:bottom="-9dp"
        android:top="-10dp">
        <shape android:shape="rectangle">
            <solid android:color="@color/profile_icon_btn_bg_color_nor" />
            <corners
                android:bottomLeftRadius="0dp"
                android:radius="6dp"
                android:topLeftRadius="0dp"
                android:topRightRadius="0dp" />
        </shape>
    </item>
</layer-list>